{
  "name": "@deltahouse/events",
  "version": "2.0.0",
  "scripts": {
    "build": "node index.js",
    "start": "esbuild src/index.js --bundle --platform=node --outfile=build/index.js --loader:.ejs=text --watch",
    "start:local": "nodemon ./src",
    "docker:hmr": "nodemon -L ./app",
    "docker:reload": "docker compose -f ../../docker-compose.yml up -d --build events --force-recreate"
  },
  "dependencies": {
    "@deltahouse/app": "file:../app",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-ts-paths": "^1.1.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.17.0",
    "react-router-dom": "^6.17.0",
    "webp-converter": "^2.3.3"
  },
  "devDependencies": {
    "cors": "^2.8.5",
    "nodemon": "^3.0.1"
  }
}
